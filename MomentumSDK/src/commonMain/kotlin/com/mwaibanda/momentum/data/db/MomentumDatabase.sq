

CREATE TABLE MomentumTransaction (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    description TEXT NOT NULL,
    date TEXT NOT NULL,
    amount REAL NOT NULL,
    seen INTEGER AS Boolean DEFAULT 0 NOT NULL
);

insertTransaction:
INSERT OR REPLACE INTO MomentumTransaction(description, date, amount, seen) VALUES (:description,:date, :amount, :seen);

getAllTransactions:
SELECT * FROM MomentumTransaction;

deleteAll:
DELETE FROM MomentumTransaction;